<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="PrimaryColor">#0d6efd</Color>
    <!--Color Primary #0d6efd -rgba(13,110,253,255) -->
    <Color x:Key="SecondaryColor">#6c757d</Color>
    <!--Color Secondary #6c757d -rgba(108,117,125,255) -->
    <Color x:Key="SuccessColor">#198754</Color>
    <!--Color Success #198754 -rgba(25,135,84,255) -->
    <Color x:Key="DangerColor">#dc3545</Color>
    <!--Color Danger #dc3545 -rgba(220,53,69,255) -->
    <Color x:Key="WarningColor">#ffc107</Color>
    <!--Color Warning #ffc107 -rgba(255,193,7,255)-->
    <Color x:Key="InfoColor">#0dcaf0</Color>
    <!--Color Info #0dcaf0 -rgba(13,202,240,255) -->
    <Color x:Key="LightColor">#fff</Color>
    <!--Color Light #ffffff -rgba(255,255,255,255) -->
    <Color x:Key="DarkColor">#212529</Color>
    <!--Color Dark #212529 -rgba(33,37,41,255) -->
    <Color x:Key="LinkColor">#0d9efe</Color>
    <!--Color Link #0d9efe -rgba(13,158,254,255) -->
    <Color x:Key="BackgroundColor">#e2f1ff</Color>
    <!--Color background #e2f1ff -rgba(226,241,255,255)-->
    <Color x:Key="FrameColor">#eeeef2</Color>
    <!--Color frame ##eeeef2 -rgba(238,238,242,255)-->

    <Color x:Key="PrimaryColorHover">#175abe</Color>
    <!--Color Primary hover #0d6efd -rgba(13,110,253,255) -->
    <Color x:Key="SecondaryColorHover">#585e63</Color>
    <!--Color Secondary hover #6c757d -rgba(108,117,125,255) -->
    <Color x:Key="SuccessColorHover">#1d6343</Color>
    <!--Color Success hover #198754 -rgba(25,135,84,255) -->
    <Color x:Key="DangerColorHover">#ab303b</Color>
    <!--Color Danger hover #dc3545 -rgba(220,53,69,255) -->
    <Color x:Key="WarningColorHover">#bd9315</Color>
    <!--Color Warning hover #ffc107 -rgba(255,193,7,255)-->
    <Color x:Key="InfoColorHover">#1d96ae</Color>
    <!--Color Info hover #0dcaf0 -rgba(13,202,240,255) -->
    <Color x:Key="LightColorHover">#cccccc</Color>
    <!--Color Light hover #ffffff -rgba(255,255,255,255) -->
    <Color x:Key="DarkColorHover">#2a2e32</Color>
    <!--Color Dark hover #212529 -rgba(33,37,41,255) -->
    <Color x:Key="LinkColorHover">#167cc0</Color>
    <!--Color Link hover #0d9efe -rgba(13,158,254,255) -->

    <SolidColorBrush x:Key="PrimaryBrush" Color="{DynamicResource PrimaryColor}" />
    <SolidColorBrush x:Key="SecondaryBrush" Color="{DynamicResource SecondaryColor}" />
    <SolidColorBrush x:Key="SuccessBrush" Color="{DynamicResource SuccessColor}" />
    <SolidColorBrush x:Key="DangerBrush" Color="{DynamicResource DangerColor}" />
    <SolidColorBrush x:Key="WarningBrush" Color="{DynamicResource WarningColor}" />
    <SolidColorBrush x:Key="InfoBrush" Color="{DynamicResource InfoColor}" />
    <SolidColorBrush x:Key="LightBrush" Color="{DynamicResource LightColor}" />
    <SolidColorBrush x:Key="DarkBrush" Color="{DynamicResource DarkColor}" />
    <SolidColorBrush x:Key="LinkBrush" Color="{DynamicResource LinkColor}" />
    <SolidColorBrush x:Key="BackgroundBrush" Color="{DynamicResource BackgroundColor}" />
    <SolidColorBrush x:Key="FrameBrush" Color="{DynamicResource FrameColor}" />

    <SolidColorBrush x:Key="PrimaryBrushHover" Color="{DynamicResource PrimaryColorHover}" />
    <SolidColorBrush x:Key="SecondaryBrushHover" Color="{DynamicResource SecondaryColorHover}" />
    <SolidColorBrush x:Key="SuccessBrushHover" Color="{DynamicResource SuccessColorHover}" />
    <SolidColorBrush x:Key="DangerBrushHover" Color="{DynamicResource DangerColorHover}" />
    <SolidColorBrush x:Key="WarningBrushHover" Color="{DynamicResource WarningColorHover}" />
    <SolidColorBrush x:Key="InfoBrushHover" Color="{DynamicResource InfoColorHover}" />
    <SolidColorBrush x:Key="LightBrushHover" Color="{DynamicResource LightColorHover}" />
    <SolidColorBrush x:Key="DarkBrushHover" Color="{DynamicResource DarkColorHover}" />
    <SolidColorBrush x:Key="LinkBrushHover" Color="{DynamicResource LinkColorHover}" />
</ResourceDictionary>